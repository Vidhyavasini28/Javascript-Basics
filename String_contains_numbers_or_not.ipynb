   "source": [
    "string = \"Hello\"\n",
    "contains_digit = False\n",
    "\n",
    "for char in string:\n",
    "    if '0' <= char <= '9':\n",
    "        contains_digit = True\n",
    "        break \n",
    "\n",
    "if contains_digit:\n",
    "    print(\"The string contains a numeric digit.\")\n",
    "else:\n",
    "    print(\"The string does not contain any numeric digits.\")\n"
